                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="outputText" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Adds plain text to the report document.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute default="0" name="relativeWidth" type="xs:float" use="optional">
                <xs:annotation>
                    <xs:documentation>
